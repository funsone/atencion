      <%= simple_form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>

        <%= f.error_notification %>
        <ul class="list-group ">
              <li class="list-group-item text-muted col-md-12" contenteditable="false"><i class="fa fa-key"></i> Datos<div class="pull-right text-warning">

              </div></li>
              <li class="list-group-item col-md-12">
        <div class="form-inputs">
          <div class="col-md-5">
            <%= f.input :email, required: false, autofocus: true, class:"form-username form-control"  %></div>
               <div class="col-md-5"><%= f.input :password, required: false, autocomplete: "off", class: "form-username form-control" %></div>
               <div class="col-md-2 text-center"> <br>
               <%= f.button :submit, "Entrar", class: "btn btn-lg" %></div>

          </div>



        </div>
        </li>
        </ul>
        </br>

        <% end %>





