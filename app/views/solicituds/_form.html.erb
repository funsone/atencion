<%= simple_form_for([@solicitud.persona, @solicitud]) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :ayuda, :label_method => lambda { |owner| "#{owner.nombre}" } %>
    <%= f.input :status %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
